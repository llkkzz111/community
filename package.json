{
  "name": "OCJ_RN_APP",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "configureJPush": "node node_modules/jpush-react-native/JPushConfiguration.js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "test": "jest",
    "ios-bundle": "react-native bundle --dev false --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "ios-dev-bundle": "react-native bundle --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "android-bundle": "react-native bundle --dev false --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "android-dev-bundle": "react-native bundle --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "web-bundle": "webpack --config web/webpack/web.prod.config.js --progress --colors",
    "web-dev": "webpack-dev-server --content-base web/public/ --config web/webpack/web.dev.config.js --port 3001 --inline --hot --colors",
    "clear-cache": "gulp clear-cache",
    "jsdoc": "jsdoc -c conf.json"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "jssha": "2.2.0",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.13",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.3",
    "react-addons-pure-render-mixin": "^15.5.0",
    "react-native": "0.43.4",
    "react-native-actionsheet": "2.0.4",
    "react-native-alphabetlistview": "^0.2.0",
    "react-native-button": "1.7.1",
    "react-native-datepicker": "1.3.2",
    "react-native-device-info": "0.9.9",
    "react-native-drawer-layout": "1.1.0",
    "react-native-easy-listview-gridview": "^1.0.3",
    "react-native-easy-toast": "^1.0.6",
    "react-native-elements": "0.6.0",
    "react-native-fetch-blob": "^0.10.6",
    "react-native-gifted-listview": "0.0.15",
    "react-native-img-cache": "^1.4.0",
    "react-native-looped-carousel": "^0.1.7",
    "react-native-navbar": "1.5.0",
    "react-native-progress": "3.1.0",
    "react-native-pull": "^2.0.2",
    "react-native-radio-buttons": "0.12.2",
    "react-native-root-toast": "1.0.3",
    "react-native-router-flux": "3.37.0",
    "react-native-scrollable-tab-view": "^0.7.4",
    "react-native-selectablesectionlistview": "^0.4.0",
    "react-native-smart-splash-screen": "2.0.0",
    "react-native-swipe-list-view": "^0.3.3",
    "react-native-swipeable": "^0.4.0",
    "react-native-swiper": "1.5.10",
    "react-native-tab-navigator": "0.3.3",
    "react-native-vector-icons": "^4.1.1",
    "react-native-viewpager": "^0.2.13",
    "react-native-webview-invoke": "^0.3.10",
    "react-redux": "4.4.5",
    "react-static-container": "^1.0.1",
    "redux": "3.6.0",
    "redux-actions": "0.12.0",
    "redux-saga": "0.12.1",
    "redux-thunk": "2.1.0",
    "reselect": "^3.0.1",
    "whatwg-fetch": "1.1.1"
  },
  "jest": {
    "preset": "jest-react-native"
  },
  "devDependencies": {
    "babel-jest": "16.0.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "1.9.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "jest": "16.0.1",
    "jest-react-native": "16.0.0",
    "jsdoc": "^3.5.3",
    "react-test-renderer": "15.3.2",
    "tui-jsdoc-template": "^1.1.0"
  }
